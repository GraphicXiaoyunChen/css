<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Group 1</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <div class="home" onclick="navigateTo('home')">Home</div>
    <div class="ellipse ellipse1" onclick="navigateTo('visiter')"></div>
    <div class="ellipse ellipse3" onclick="navigateTo('tracking')"></div>
    <div class="rectangle-div" onclick="navigateTo('artwork2')"></div>

    <script>
        function navigateTo(page) {
            switch(page) {
                case 'home':
                    window.location.href = 'home.html';
                    break;
                case 'visiter':
                    window.location.href = 'visiter.html';
                    break;
                case 'tracking':
                    window.location.href = 'tracking.html';
                    break;
                case 'artwork2':
                    window.location.href = 'artwork2.html';
                    break;
            }
        }
    </script>
</body>
</html>
